 def subsets_of_size_k(nums, k):
     if k == 0:
         return [[]]
     
     result = []
     for i in range(len(nums)):
         num = nums[i]
         remaining_nums = nums[i+1:]
         smaller_subsets = subsets_of_size_k(remaining_nums, k - 1)
         for subset in smaller_subsets:
             result.append([num] + subset)
     
     return result
 
