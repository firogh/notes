grep cleanup-dentry-cache.sh messages-2025-03-31-1743372000.txt |grep  'Current\|after'  | awk 'BEGIN{i=0;prev=0; print "time\t\t\t\t\t", "current","left-over","diff";}{if (i%2 == 1) print time, $1, prev, $NF, prev - $NF; else prev= $NF ; time=$1 ; i = i+1;}' | awk '
 {
     split($1, part1, /[T+:.-]/);
     hour1 = part1[4];
     min1 = part1[5];
     sec1 = part1[6];
     micro1 = part1[7];
     tz1 = part1[8] * 3600 + part1[9] * 60;
     time1 = hour1 * 3600 + min1 * 60 + sec1 + micro1 / 1000000 - tz1;
     split($2, part2, /[T+:.-]/);
     hour2 = part2[4];
     min2 = part2[5];
     sec2 = part2[6];
     micro2 = part2[7];
     tz2 = part2[8] * 3600 + part2[9] * 60;
     time2 = hour2 * 3600 + min2 * 60 + sec2 + micro2 / 1000000 - tz2;
     time_diff = time2 - time1;
     printf "%s %.6f\n", $0, time_diff;
 }' 
